{
    "name": "nicolaskion/sde",
    "description": "SDE Parser for EVE Online and Laravel",
    "keywords": [
        "php",
        "eve",
        "package",
        "sde",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Kion",
            "email": "nicolaskion07@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "ext-zip": "*",
        "illuminate/contracts": "^10||^11",
        "laravel/framework": "^11",
        "symfony/yaml": "^7.2"
    },
    "require-dev": {
        "laravel/pint": "^1.13.7",
        "pestphp/pest": "^2.28.1",
        "phpstan/phpstan": "^2.1.1",
        "rector/rector": "^2.0.7",
        "symfony/var-dumper": "^6.4.0|^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "NicolasKion\\SDE\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "refacto": "rector",
        "lint": "pint",
        "test:refacto": "rector --dry-run",
        "test:lint": "pint --test",
        "test:types": "phpstan analyse --ansi",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:refacto",
            "@test:lint",
            "@test:types",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NicolasKion\\SDE\\SDEProvider"
            ]
        }
    }
}
